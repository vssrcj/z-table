{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": [ "eslint:recommended", "plugin:react/recommended", "standard" ],
	"plugins": [
		"standard",
		"promise",
		"react"
	],
	"parser": "babel-eslint",
	"rules": {		 
		"indent": [
			"error",
			"tab",
			{ "SwitchCase": 1 }
		],
		"no-class-assign": 0,
		"no-tabs": 0,
		"react/display-name": 0,
		"react/no-unknown-property": 0
	}
}
